@model AdmissionWebsiteFinal.Models.AdmissionEntryViewModel

@{
    ViewData["Title"] = "EntryDetails";
}

<h1>Entry Details</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contestant.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contestant.Id)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.EntryScore)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EntryScore)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contestant.FirstName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contestant.FirstName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contestant.LastName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contestant.LastName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contestant.PhoneNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contestant.PhoneNumber)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contestant.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contestant.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contestant.Adress)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contestant.Adress)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Contestant.DateOfBirth)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contestant.DateOfBirth)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Employee)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EmployeeId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateCreated)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateCreated)
        </dd>
    </dl>
</div>
<div>
    <h4>Options</h4>
    @if (Model.Options != null)
    {
        <table class="table text-nowrap table-bordered table-striped">
            <tbody>
                @foreach (var item in Model.Options)
                {
                    <tr>
                        <td>
                            @(item.Name)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    }
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
